{
    "name": "@aics/simularium-viewer",
    "version": "1.2.7",
    "description": "An npm package to view simulations.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "browser": "dist/index.js",
    "types": "type-declarations",
    "scripts": {
        "lint": "eslint src/**/*.ts src/**/*.tsx",
        "test": "jest src/**/*.test.ts",
        "typeCheck": "tsc -p tsconfig.json --noEmit",
        "generateTypes": "tsc -p tsconfig.base.json --emitDeclarationOnly",
        "bundle": "BABEL_ENV=es NODE_ENV=production webpack --config webpack.config.js",
        "transpileES": "TS_NODE_PROJECT=tsconfig.es.json BABEL_ENV=es babel src --out-dir es --extensions .ts,.tsx --ignore src/**/*.test.ts",
        "transpileCommonJs": "TS_NODE_PROJECT=tsconfig.commonjs.json babel src --out-dir lib --extensions .ts,.tsx --ignore src/**/*.test.ts",
        "processCSS": "postcss src/viewport/index.css --output style/style.css",
        "build": "npm run generateTypes & npm run bundle & npm run transpileES & npm run transpileCommonJs & npm run processCSS"
    },
    "files": [
        "es",
        "lib",
        "dist",
        "style",
        "type-declarations",
        "README.md",
        "package.json"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "~7.2",
        "@babel/core": "~7.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "~7.3",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "~7.1",
        "@types/jest": "^26.0.3",
        "@types/lodash": "^4.14.158",
        "@types/react": "^16.8.25",
        "@typescript-eslint/eslint-plugin": "~3.4.0",
        "@typescript-eslint/parser": "~3.4.0",
        "babel-loader": "~8.0",
        "babel-plugin-const-enum": "^1.0.1",
        "copy-webpack-plugin": "~5.0.4",
        "css-loader": "^3.2.0",
        "cssnano": "^4.1.10",
        "eslint": "^7.3.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-react": "^7.20.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "jest": "^26.1.0",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.8.0",
        "nyc": "~15.0",
        "postcss": "^7.0.17",
        "postcss-cli": "^6.1.3",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^1.16.1",
        "rimraf": "~2.6",
        "ts-jest": "^26.1.1",
        "ts-node": "~8.10.2",
        "typescript": "~3.9.5",
        "webpack": "~4.29",
        "webpack-cli": "~3.2",
        "webpack-dev-server": "^3.7.2",
        "worker-loader": "^2.0.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.29",
        "@fortawesome/free-solid-svg-icons": "^5.13.1",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "comlink": "^4.3.0",
        "dat.gui": "^0.7.6",
        "js-logger": "^1.6.0",
        "parse-pdb": "^1.0.0",
        "react": "~16.9.0",
        "react-dom": "^16.9.0",
        "three": "^0.110.0"
    }
}
